		"prettier": "^2.5.1",
		"typescript": "^4.6.2",
		"vite": "^2.8.6",
		"vite-plugin-externals": "^0.4.0"
	},
	"exports": {
		".": {
			"import": "./build/esm/index.js",
			"require": "./build/cjs/index.js"
		}
	},
	"lint-staged": {
		"vite.config.ts": [
			"prettier --write"
