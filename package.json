{
	"type": "module",
	"name": "cesium-draw-core",
	"author": {
		"name": "openspacing"
	},
	"packageManager": "pnpm@6.30.0+",
	"repository": {
		"github": "https://github.com/openspacing/cesium-draw-core"
	},
	"keywords": [
		"cesium",
		"draw",
		"tool"
